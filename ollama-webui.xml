<?xml version="1.0"?>
<Container version="2">
  <Name>ollama-webui</Name>
  <Repository>ghcr.io/ollama-webui/ollama-webui:main</Repository>
  <Registry>https://github.com/ollama-webui/ollama-webui/pkgs/container/ollama-webui/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/ollama-webui/ollama-webui/issues</Support>
  <Project>https://github.com/ollama-webui/ollama-webui</Project>
  <Overview>A User-Friendly Web Interface for Chat Interactions</Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <TemplateURL/>
  <Icon>https://avatars.githubusercontent.com/u/151674099</Icon>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Ollama Base URL" Target="OLLAMA_API_BASE_URL" Default="" Mode="" Description="Your ollama server url: http://example.com/api" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Web Interface" Target="8080" Default="3000" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">3000</Config>
</Container>
